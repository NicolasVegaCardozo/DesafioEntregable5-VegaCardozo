<h1>Hola {{usuario.nombre}}</h1>

<ul>
    {{#each products}}
    <li class="product-item">{{this.id}}</li>
    <li>{{this.title}}</li>
    <li>{{this.description}}</li>
    <li>{{this.price}}</li>
    <li>{{this.status}}</li>
    <li>{{this.code}}</li>
    <li>{{this.stock}}</li>
    {{/each}}
</ul>